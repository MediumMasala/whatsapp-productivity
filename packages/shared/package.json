      "types": "./dist/constants/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "chrono-node": "^2.7.6",
    "date-fns": "^3.3.1",
    "date-fns-tz": "^3.1.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
